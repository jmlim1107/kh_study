import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;

class Solution {
    public int solution(int n) {
        int answer = 0;
        
        // ArrayList 객체 생성(자동 배열생성을 위해)
        ArrayList<Integer> list = new ArrayList<Integer>();
        
        // 조건에 성립하면 배열에 추가
        for(int i =1; i<= n; i++){
            if(n%i == 1){
                list.add(i);
            }
        }
        
        // 오름차순 정렬
        Collections.sort(list);

        // 가장 작은 인덱스안의 값 얻어오기 
        return list.get(0);
    }
}
